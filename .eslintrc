{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": [
    "standard",
    "standard-react",
    "prettier/standard",
    "plugin:prettier/recommended"
  ],
  "parser": "babel-eslint",
  "plugins": ["react-hooks"],
  "rules": {
    "import/first": "off",
    "no-case-declarations": "off",
    "react/prop-types": "off",
    "react-hooks/rules-of-hooks": "error",
    "react-hooks/exhaustive-deps": "error"
  }
}
